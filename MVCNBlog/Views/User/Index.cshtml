@model IEnumerable<MVCNBlog.ViewModels.UserViewModel>
@{
    ViewBag.Title = "Home Page";
}

<h2>All users</h2>
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayTextFor(modelItem => item.CreationDate)
            </td>
        </tr>
    }
</table>
